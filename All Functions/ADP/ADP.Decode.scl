FUNCTION "ADP.Decode" : "ADP: Message Formats: Encoded"
{ S7_Optimized_Access := 'TRUE' }
VERSION : 0.1
   VAR_INPUT 
      codes : "ADP: Message Formats: Encoded";
      adp_decoded : "ADP: Decoded";
      adp_encoded : "ADP: Encoded: Formatting";
   END_VAR

   VAR_TEMP 
      _ret : "ADP: Message Formats: Encoded";
   END_VAR


BEGIN
	#_ret.color := "ADP: Decode: Color"(color := #codes.color,
	                                    encoded := #adp_encoded.color,
	                                    decoded := #adp_decoded.color);
	#_ret.flash := "ADP: Decode: Flashing"(flash := #codes.flash,
	                                       decoded := #adp_decoded.flash,
	                                       encoded := #adp_encoded.flashing);
	#_ret.horizontal_alignment := "ADP: Decode: Horizontal Alignment"(horizontal_alignment := #codes.horizontal_alignment,
	                                                                  decoded := #adp_decoded.horizontal_alignment,
	                                                                  encoded := #adp_encoded.horizontal_alignment);
	#_ret.scroll_speed := "ADP: Decode: Scroll Speed"(scroll_speed := #codes.scroll_speed,
	                                                  decoded := #adp_decoded.scroll_speed,
	                                                  encoded := #adp_encoded.scroll_speed);
	#_ret.size_and_face := "ADP: Decode: Size and Face"(size_and_face := #codes.size_and_face,
	                                                    decoded := #adp_decoded.size_and_face,
	                                                    encoded := #adp_encoded.size_and_face);
	#_ret.vertical_alignment := "ADP: Decode: Vertical Alignment"(vertical_alignment := #codes.vertical_alignment,
	                                                              decoded := #adp_decoded.vertical_alignment,
	                                                              encoded := #adp_encoded.vertical_alignment);
	#"ADP: Decode" := #_ret;
END_FUNCTION

